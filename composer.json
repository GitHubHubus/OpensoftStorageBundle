{
  "name": "opensoft/storage-bundle",
  "description": "An opinionated storage system for Opensoft",
  "keywords": ["storage", "file", "gaufrette", "cloud"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Richard Fullmer",
      "email": "richardfullmer@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Opensoft\\StorageBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": ">=5.6",
    "symfony/framework-bundle": "^2.7|^3.0",
    "twig/twig": "^1.25|^2.0",
    "doctrine/orm": "^2.5",
    "knplabs/gaufrette": "^0.3.1",
    "lexik/form-filter-bundle": "^5.0",
    "symfony/validator": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zenddiagnostics": "^1.0",
    "aws/aws-sdk-php": "^3.29"
  }
}
